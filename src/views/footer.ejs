<% if (user) { %> 
    <% if (user.rol === "admin") { %>
    <div class="sidebarDerecha" id="sidebarAdmin">
        <h4>Usuarios</h4>
        <% i = 0; %>
        <% usuarios.forEach((username)=>{ %>
          <% if (i < 8) {%> 
            <div class="balance">
              <div class="info">
                  <h5><%= username.dataValues.username%></h5>
                  <span><%= username.dataValues.last_login.toISOString()%></span>
              </div>
              </div>
          <% } %>
          <% i++ %>
        <% }) %>
    </div>
    <% } else if (user.rol === "user") { %>
        <div class="sidebarDerecha" id="sidebarDerechaInicio">
            <div class="titulo">
                <h4>Account Balance</h4>
            </div>
            <div class="balance">
              <!-- <i class="fas fa-dollar icon"></i> -->
            
              <i id="dolar" class='bx bx-dollar'></i>
              <div class="info">
                <h5>Dollar</h5>
                <span></i><%= user.saldo%> $<i class='bx bx-dollar'></i></span>
              </div>
            </div>
            
            <div class="balance">
            <i id = "inr" class='bx bx-rupee' ></i>
              <div class="info">
                <h5>INR</h5>
                <span></i><%= user.saldo * 83,29%> ₹<i class='bx bx-rupee' ></i></span>
              </div>
            </div>
      
            <div class="balance">
            <i id = "euro" class='bx bx-euro' ></i></i>
              <div class="info">
                <h5>Euro</h5>
                <span></i><%= user.saldo * 0.91%> €<i class='bx bx-euro' ></i></span>
              </div>
            </div>
      
            <div class="balance">
            <i id="yenes"class='bx bx-yen'></i></i>
              <div class="info">
                <h5>JPY</h5>
                <span></i><%= user.saldo * 143%> ¥ <i class='bx bx-yen'></i></span>
              </div>
            </div>
      
            <div class="balance">
                <i id= "pound"class='bx bx-pound' ></i>
              <div class="info">
                <h5>Pound</h5>
                <span></i> <%= user.saldo * 0.79%> £ <i class='bx bx-pound'></i></span>
              </div>
            </div>
      
          </div>
        </div>
        <% } %>
    </div>
    </body>
    </html> 
¡<% } %>